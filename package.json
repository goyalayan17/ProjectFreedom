{
  "name": "freedom.dashboard",
  "version": "1.0.0",
  "description": "create a dashboard to learn the react-redux",
  "sideEffects": false,
  "main": "dist/public_apis.js",
  "module": "dist/public_apis.js",
  "types": "dist/public_apis.d.ts",
  "scripts": {
    "build": "npm run export",
    "export": "npm run build-ts",
    "build-ts": "node_modules/.bin/ttsc -p tsconfig.json --outDir dist",
    "test": "node scripts/test.js --env=jest-environment-jsdom --coverage",
    "storybook": "start-storybook -p 9001"
  },
  "author": "Ayan Goyal",
  "license": "ISC",
  "jest": {
    "collectCoverageFrom":[
      "src/**/*.{ts,tsx}",
      "!src/public_apis.ts",
      "!src/{domain}/index.ts"
    ],
    "coverageDirectory": "coverge",
    "coverageReporters": [
      "json",
      "cobertura",
      "lcov",
      "text"
    ],
    "globals": {
      "ts-jest": {
        "diagnostics": false,
        "tsConfig": "tsconfig.test.json"
      }
    },
    "moduleFileExtensions": [
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "web.js",
      "js",
      "mjs",
      "web.jsx",
      "jsx",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "src/(.*)": "<rootDir>/src/$1",
      "react-native$": "react-native-web",
      "\\.(css|scss|less)$": "identity-obj-proxy",
      "\\.worker.entry.js": "<rootDir>/tests/__mocks__/workerMock.js"
    },
    "testRunner": "jest-circus/runner",
    "testEnvironment": "jest-environment-jsdom",
    "testMatch": [
      "**/tests/**/?(*.)(spec|test).ts?(x)"
    ],
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest",
      "^.+\\.mjs$": "babel-jest",
      "^.+\\.(ts|tsx)$": "<rootDir>/config/jest/typescriptTransform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@emotion/babel-preset-css-prop": "^11.2.0",
    "@emotion/react": "^11.4.0",
    "@storybook/addons": "^6.3.4",
    "@storybook/react": "^6.3.4",
    "@storybook/theming": "^6.3.4",
    "@types/enzyme": "^3.10.9",
    "@types/jest": "^26.0.24",
    "@types/react": "^17.0.15",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.3",
    "babel-jest": "^27.0.6",
    "babel-plugin-emotion": "^11.0.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "enzyme": "^3.11.0",
    "identity-obj-proxy": "^3.0.0",
    "install": "^0.13.0",
    "jest": "^27.0.6",
    "jest-environment-jsdom": "^27.0.6",
    "jest-environment-jsdom-sixteen": "^2.0.0",
    "jest-enzyme": "^7.1.2",
    "jsdom": "^17.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "ts-jest": "^27.0.4",
    "ts-loader": "^8.0.17",
    "ttypescript": "^1.5.12",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "@emotion/core": "^11.0.0",
    "@emotion/styled": "^11.3.0",
    "@material-ui/core": "^4.12.2",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "@redux-saga/core": "^1.1.3",
    "@reduxjs/toolkit": "^1.6.1",
    "axios": "^0.21.1",
    "npm": "^7.20.2",
    "react-redux": "^7.2.4",
    "webpack-cli": "^4.7.2"
  }
}
